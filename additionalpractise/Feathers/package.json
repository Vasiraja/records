{
  "name": "basepro",
  "version": "0.0.0",
  "private": true,
  "description": "simple basepro named project to testing api",
  "keywords": [
    "feathers"
  ],
  "homepage": "",
  "license": "ISC",
  "author": "undefined",
  "contributors": [],
  "type": "commonjs",
  "main": "lib/client",
  "directories": {
    "test": "test"
  },
  "files": [
    "lib/client.js",
    "lib/**/*.d.ts",
    "lib/**/*.shared.js"
  ],
  "scripts": {
     "dev": "nodemon -x ts-node src/index.ts",
    "compile": "shx rm -rf lib/ && tsc",
    "start": "node src/",
    "prettier": "npx prettier \"**/*.ts\" --write",
    "mocha": "cross-env NODE_ENV=test mocha test/ --require ts-node/register --recursive --extension .ts --exit",
    "test": "npm run mocha",
    "bundle:client": "npm run compile && npm pack --pack-destination ./public"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "^5.0.37",
    "@feathersjs/authentication": "^5.0.37",
    "@feathersjs/authentication-client": "^5.0.37",
    "@feathersjs/configuration": "^5.0.37",
    "@feathersjs/errors": "^5.0.37",
    "@feathersjs/express": "^5.0.37",
    "@feathersjs/feathers": "^5.0.37",
    "@feathersjs/mongodb": "^5.0.37",
    "@feathersjs/schema": "^5.0.37",
    "@feathersjs/socketio": "^5.0.37",
    "@feathersjs/transport-commons": "^5.0.37",
    "@feathersjs/typebox": "^5.0.37",
    "compression": "^1.8.1",
    "mongodb": "^6.19.0",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@feathersjs/cli": "^5.0.37",
    "@feathersjs/rest-client": "^5.0.37",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.10.0",
    "axios": "^1.13.2",
    "cross-env": "^10.1.0",
    "mocha": "^11.7.5",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "shx": "^0.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">= 24.12.0"
  },
  "feathers": {
    "language": "ts",
    "packager": "npm",
    "database": "mongodb",
    "framework": "express",
    "transports": [
      "rest",
      "websockets"
    ],
    "schema": "typebox"
  }
}
